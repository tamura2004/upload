<p id="notice"><%= notice %></p>

<h1>Plans</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Group name</th>
      <th>Project number</th>
      <th>Project name</th>
      <th>Accuracy</th>
      <th>Dept name</th>
      <th>Group name</th>
      <th>Sub number</th>
      <th>System name</th>
      <th>Contract type</th>
      <th>Company name</th>
      <th>Member rank</th>
      <th>Unit price</th>
      <th>Manhour last month landing</th>
      <th>Manhour performance</th>
      <th>Manhour development plan</th>
      <th>Manhour landing</th>
      <th>Manhour divergence</th>
      <th>Manhour ernings</th>
      <th>Money last month landing</th>
      <th>Money performance</th>
      <th>Money development plan</th>
      <th>Money landing</th>
      <th>Money divergence</th>
      <th>Money ernings</th>
      <th>Cost rate plan</th>
      <th>Cost rate landing</th>
      <th>Gross profit plan</th>
      <th>Gross profit landing</th>
      <th>Gross profit divergence</th>
      <th>To be confirmed</th>
      <th>M1</th>
      <th>M2</th>
      <th>M3</th>
      <th>M4</th>
      <th>M5</th>
      <th>M6</th>
      <th>M7</th>
      <th>M8</th>
      <th>M9</th>
      <th>M10</th>
      <th>M11</th>
      <th>M12</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plans.each do |plan| %>
      <tr>
        <td><%= plan.category %></td>
        <td><%= plan.group_name %></td>
        <td><%= plan.project_number %></td>
        <td><%= plan.project_name %></td>
        <td><%= plan.accuracy %></td>
        <td><%= plan.dept_name %></td>
        <td><%= plan.group_name %></td>
        <td><%= plan.sub_number %></td>
        <td><%= plan.system_name %></td>
        <td><%= plan.contract_type %></td>
        <td><%= plan.company_name %></td>
        <td><%= plan.member_rank %></td>
        <td><%= plan.unit_price %></td>
        <td><%= plan.manhour_last_month_landing %></td>
        <td><%= plan.manhour_performance %></td>
        <td><%= plan.manhour_development_plan %></td>
        <td><%= plan.manhour_landing %></td>
        <td><%= plan.manhour_divergence %></td>
        <td><%= plan.manhour_ernings %></td>
        <td><%= plan.money_last_month_landing %></td>
        <td><%= plan.money_performance %></td>
        <td><%= plan.money_development_plan %></td>
        <td><%= plan.money_landing %></td>
        <td><%= plan.money_divergence %></td>
        <td><%= plan.money_ernings %></td>
        <td><%= plan.cost_rate_plan %></td>
        <td><%= plan.cost_rate_landing %></td>
        <td><%= plan.gross_profit_plan %></td>
        <td><%= plan.gross_profit_landing %></td>
        <td><%= plan.gross_profit_divergence %></td>
        <td><%= plan.to_be_confirmed %></td>
        <td><%= plan.m1 %></td>
        <td><%= plan.m2 %></td>
        <td><%= plan.m3 %></td>
        <td><%= plan.m4 %></td>
        <td><%= plan.m5 %></td>
        <td><%= plan.m6 %></td>
        <td><%= plan.m7 %></td>
        <td><%= plan.m8 %></td>
        <td><%= plan.m9 %></td>
        <td><%= plan.m10 %></td>
        <td><%= plan.m11 %></td>
        <td><%= plan.m12 %></td>
        <td><%= link_to 'Show', plan %></td>
        <td><%= link_to 'Edit', edit_plan_path(plan) %></td>
        <td><%= link_to 'Destroy', plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

